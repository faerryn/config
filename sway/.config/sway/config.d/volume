set $aucatctl_wrapper ~/.config/sway/scripts/aucatctl.sh
bindsym XF86AudioRaiseVolume       exec $aucatctl_wrapper master +8
bindsym Shift+XF86AudioRaiseVolume exec aucatctl          master=127
bindsym XF86AudioLowerVolume       exec $aucatctl_wrapper master -8
bindsym Shift+XF86AudioLowerVolume exec aucatctl          master=0
bindsym XF86AudioMute              exec aucatctl          master=0
